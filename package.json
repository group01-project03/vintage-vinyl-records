{
  "name": "vintage-vinyl-records",
  "version": "1.0.0",
  "description": "[![License: MIT](https://img.shields.io/badge/License-MIT-yellow.svg)](https://opensource.org/licenses/MIT)",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/group01-project03/vintage-vinyl-records.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/group01-project03/vintage-vinyl-records/issues"
  },
  "homepage": "https://github.com/group01-project03/vintage-vinyl-records#readme",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "concurrently": "^6.2.1",
    "mongoose": "^6.0.6"
  }
}
